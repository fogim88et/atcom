						<sp:info>
							<op:pbTi/>
							<op:res>
								<sp:txt>
									<op:txt>
										<sc:para sc:id="t85">La maintenance a besoin de préparer au mieux son intervention.</sc:para>
										<sc:para>Les améliorations suivantes peuvent être apportées :</sc:para>
										<sc:itemizedList sc:id="t86">
											<sc:listItem sc:id="t87">
												<sc:para sc:id="t88">une description précise des symptômes de la panne.</sc:para>
											</sc:listItem>
											<sc:listItem sc:id="t89">
												<sc:para sc:id="t90">le nom de l'émetteur.</sc:para>
											</sc:listItem>
											<sc:listItem sc:id="t93">
												<sc:para sc:id="t94">la référence du véhicule</sc:para>
											</sc:listItem>
											<sc:listItem sc:id="t95">
												<sc:para sc:id="t96">le kilométrage du véhicule</sc:para>
											</sc:listItem>
											<sc:listItem sc:id="t97">
												<sc:para sc:id="t98">le délai demandé</sc:para>
											</sc:listItem>
										</sc:itemizedList>
