					<op:txt>
						<sc:para xml:space="preserve" sc:id="t31"><sc:uLink role="glos" sc:id="t32" sc:refUri="/P_op/MP/4_prod/besoin_net.ref">BN</sc:uLink> =0-100-0+0</sc:para>
						<sc:para xml:space="preserve" sc:id="t36">SD (jour 6) = 0-100 + 0 + 100 = 0</sc:para>
