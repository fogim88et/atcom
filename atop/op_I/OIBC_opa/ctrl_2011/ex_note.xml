<sc:inlineStyle role="emp" sc:id="t83">qB.2. Nb de note par matière.</sc:inlineStyle>
</sc:para>
								<sc:para xml:space="preserve" sc:id="t84">Formule en H3</sc:para>
							</op:txt>
						</sp:txt>
					</op:res>
				</sp:desc>
			</op:practUcQ>
		</sp:quest>
